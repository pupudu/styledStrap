{
  "name": "stylestrap",
  "version": "0.5.0",
  "description": "Bootstrap based components library built using styled components",
  "main": "dist/index.js",
  "module": "dist/stylestrap.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pupudu/stylestrap.git"
  },
  "keywords": [
    "bootstrap",
    "react",
    "styled",
    "components"
  ],
  "author": "Pubudu Dodangoda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pupudu/stylestrap/issues"
  },
  "homepage": "https://github.com/pupudu/stylestrap#readme",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --env=jsdom",
    "lint": "tsdx lint src",
    "docz": "docz dev",
    "docz:build": "docz build"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "reactstrap": "^8.0.0"
  },
  "peerDependencies": {
    "bootstrap": ">= 4",
    "polished": ">= 2",
    "prop-types": ">= 15",
    "react": ">= 16",
    "react-dom": ">= 16",
    "react-select": ">= 2",
    "reactstrap": ">=8.0.0",
    "styled-components": ">= 4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.19",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "bootstrap": "^4.1.3",
    "cssnano": "^4.1.10",
    "docz": "^1.3.2",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.1.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "polished": "^3.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.13",
    "react-select": "^3.0.3",
    "rollup-plugin-postcss": "^2.0.3",
    "styled-components": "^4.1.2",
    "tsdx": "^0.9.2",
    "tslib": "^1.10.0",
    "typescript": "^3.6.3"
  }
}
