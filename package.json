{
  "name": "stylestrap",
  "version": "0.2.4",
  "description": "Bootstrap based components library built using styled components",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "start": "docz dev",
    "docz:build": "docz build",
    "build": "STYLED_ENGINE=styled-components BABEL_ENV=rollup rollup -c",
    "lint": "eslint --fix src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pupudu/stylestrap.git"
  },
  "keywords": [
    "bootstrap",
    "react",
    "styled",
    "components"
  ],
  "author": "Pubudu Dodangoda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pupudu/stylestrap/issues"
  },
  "homepage": "https://github.com/pupudu/stylestrap#readme",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@svgr/rollup": "^4.2.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "bootstrap": "^4.1.3",
    "css-loader": "^2.1.1",
    "docz": "^1.1.0",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.1.0",
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^2.3.0",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.12.0",
    "polished": "^3.4.0",
    "prettier": "^1.14.2",
    "prop-types": "^15.6.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^4.8.8",
    "react-select": "^3.0.3",
    "rollup": "^1.10.1",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.21.0",
    "rollup-plugin-url": "^2.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "styled-components": "^4.1.2"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "reactstrap": "^8.0.0"
  },
  "peerDependencies": {
    "bootstrap": ">= 4",
    "polished": ">= 2",
    "prop-types": ">= 15",
    "react": ">= 16",
    "react-dom": ">= 16",
    "react-select": ">= 2",
    "reactstrap": ">=8.0.0",
    "styled-components": ">= 4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "yarn lint",
      "git add"
    ]
  }
}
